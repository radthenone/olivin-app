{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@api/*": [
                "./api/*"
            ],
            "@modules/*": [
                "./modules/*"
            ],
            "@components/*": [
                "./components/*"
            ],
            "@hooks/*": [
                "./hooks/*"
            ],
            "@lib/*": [
                "./lib/*"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "@types/*": [
                "./types/*"
            ],
            "@styles/*": [
                "./styles/*"
            ],
            "@assets/*": [
                "./assets/*"
            ],
            "@config/*": [
                "./config/*"
            ],
            "@constants/*": [
                "./constants/*"
            ],
            "@contexts/*": [
                "./contexts/*"
            ],
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.web.ts",
        "**/*.web.tsx",
        "**/*.android.ts",
        "**/*.android.tsx",
        "**/*.ios.ts",
        "**/*.ios.tsx",
        "**/*.native.ts",
        "**/*.native.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}